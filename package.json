{
  "name": "fastify-typescript-starter",
  "version": "4.2.0",
  "description": "Fastify TypeScript starter repository",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bootstrap/server.js",
    "dev": "dotenv -e .env -- cross-env NODE_ENV=development tsx watch source/bootstrap/server.ts",
    "dev:watch": "dotenv -e .env -- cross-env NODE_ENV=development node --watch source/bootstrap/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"source/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "author": "Mohammed Ghazy <officialmohammedtalaat.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.7",
    "redis": "^5.10.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "fastify-tsconfig": "^3.0.0",
    "prettier": "^3.7.3",
    "prisma": "^7.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
